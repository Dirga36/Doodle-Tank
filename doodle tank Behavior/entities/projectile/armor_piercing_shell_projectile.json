{
	"format_version": "1.21.0",
	"minecraft:entity": {
		"description": {
			"identifier": "dt:armor_piercing_shell_projectile",
			"is_spawnable": false,
			"is_summonable": true,
			"is_experimental": false,
			"runtime_identifier": "minecraft:snowball"
		},
		"components": {
			"minecraft:collision_box": {
				"width": 0.5,
				"height": 0.5
			},
			"minecraft:physics": {
				"has_collision": true,
				"has_gravity": true
			},
			"minecraft:conditional_bandwidth_optimization": {
				"default_values": {
					"max_optimized_distance": 80,
					"max_dropped_ticks": 10,
					"use_motion_prediction_hints": true
				}
			},
			"minecraft:tick_world": {
				"never_despawn": true
			},
			"minecraft:timer": {
				"looping": true,
				"time": 10,
				"time_down_event": {
					"event": "minecraft:explode",
					"target": "self"
				}
			}
		},
		"events": {
			"minecraft:explode": {
				"add": {
					"component_groups": [
						"minecraft:exploding"
					]
				},
				"queue_command": {
					"command": [
						"particle af:explosion_dust ~ ~-1 ~",
						"playsound tank.hit @p[r=5]"
					]
				}
			}
		},
		"component_groups": {
			"minecraft:exploding": {
				"minecraft:explode": {
					"fuse_length": 0,
					"fuse_lit": true,
					"power": 2,
					"causes_fire": false,
					"breaks_blocks": true,
					"destroy_affected_by_griefing": true
				}
			}
		}
	}
}